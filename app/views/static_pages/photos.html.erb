<main class="container content-container">

<!-- cover photo -->
  <header>
    <div class="row cover-photo-row">

      <div class="col-xs-12 cover-box">

        <div class="row spacer"></div>

        <div class="row user-name">
          <h1>Harry Potter</h1>
        </div>

      </div>

    </div>  <!-- cover photo row -->




